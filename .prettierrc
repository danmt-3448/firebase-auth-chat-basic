{
 "semi": false,
 "singleQuote": true,
 "tabWidth": 2,
 "printWidth": 80,
 "trailingComma": "all",
 "bracketSpacing": false,
 "jsxBracketSameLine": false,
 "arrowParens": "always",
 "endOfLine": "auto",
 "overrides": [
   {
     "files": "*.json",
     "options": {
       "printWidth": 120,
       "tabWidth": 4
     }
   },
   {
     "files": "*.css",
     "options": {
       "singleQuote": false
     }
   }
 ]
}
